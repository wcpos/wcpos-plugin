{
	"name": "woocommerce-pos",
	"version": "1.0.0-beta",
	"description": "A simple front-end for taking WooCommerce orders at the Point of Sale.",
	"main": "index.js",
	"sideEffects": [
		"*.css"
	],
	"scripts": {
		"wp-env": "wp-env",
		"start": "wp-env start --xdebug=debug --debug",
		"stop": "wp-env stop",
		"cli": "wp-env run cli",
		"wp": "wp-env run cli wp",
		"pretest": "wp-env start --xdebug=coverage && wp-env run composer 'install --no-interaction'",
		"lint:php": "wp-env run composer run-script lint",
		"lint": "run-s lint:*",
		"test:unit:php": "wp-env run phpunit 'phpunit -c /var/www/html/wp-content/plugins/woocommerce-pos/.phpunit.xml.dist --verbose'",
		"test:unit": "run-s test:unit:*",
		"test": "run-s test:*",
		"postinstall": "composer install",
		"build:docs": "rm -rf hookdocs/ && jsdoc -c hookdoc-conf.json",
		"build:js": "NODE_ENV=production ./node_modules/.bin/webpack",
		"start:js": "./node_modules/.bin/webpack --watch",
		"precommit": "composer dump-autoload --no-dev",
		"prepare": "husky install"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/wcpos/woocommerce-pos.git"
	},
	"keywords": [
		"wordpress",
		"woocommerce",
		"ecommerce",
		"point-of-sale"
	],
	"author": "kilbot",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/wcpos/woocommerce-pos/issues"
	},
	"homepage": "https://wcpos.com",
	"dependencies": {
		"@heroicons/react": "2.0.13",
		"@reach/auto-id": "0.18.0",
		"@reach/checkbox": "0.18.0",
		"@reach/combobox": "0.18.0",
		"@reach/dialog": "0.18.0",
		"@reach/listbox": "0.18.0",
		"@reach/portal": "0.18.0",
		"@reach/tooltip": "0.18.0",
		"@wordpress/api-fetch": "6.20.0",
		"@wordpress/element": "4.20.0",
		"@wordpress/i18n": "4.22.0",
		"@wordpress/url": "3.23.0",
		"classnames": "2.3.2",
		"lodash": "4.17.21",
		"react": "18.2.0",
		"react-beautiful-dnd": "13.1.1",
		"react-dom": "18.2.0",
		"react-error-boundary": "3.1.4"
	},
	"devDependencies": {
		"@babel/core": "7.20.5",
		"@babel/plugin-transform-runtime": "7.19.6",
		"@babel/preset-env": "7.20.2",
		"@babel/preset-react": "7.18.6",
		"@babel/preset-typescript": "7.18.6",
		"@babel/runtime": "7.20.6",
		"@svgr/webpack": "6.5.1",
		"@types/jest": "29.2.4",
		"@types/lodash": "4.14.191",
		"@types/react": "18.0.26",
		"@types/react-beautiful-dnd": "13.1.3",
		"@types/react-dom": "18.0.9",
		"@typescript-eslint/eslint-plugin": "5.46.0",
		"@typescript-eslint/parser": "5.46.0",
		"@wcpos/eslint-config": "^1.0.19",
		"@wordpress/babel-plugin-makepot": "5.6.0",
		"@wordpress/env": "5.7.0",
		"autoprefixer": "10.4.13",
		"babel-loader": "9.1.0",
		"css-loader": "6.7.2",
		"eslint": "8.29.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-jest": "27.1.6",
		"eslint-plugin-prettier": "4.2.1",
		"eslint-plugin-react": "7.31.11",
		"eslint-plugin-react-hooks": "4.6.0",
		"eslint-webpack-plugin": "3.2.0",
		"file-loader": "6.2.0",
		"fork-ts-checker-webpack-plugin": "7.2.13",
		"html-webpack-plugin": "5.5.0",
		"husky": "8.0.2",
		"jest": "29.3.1",
		"jsdoc": "4.0.0",
		"mini-css-extract-plugin": "2.7.2",
		"node-sass": "8.0.0",
		"npm-run-all": "4.1.5",
		"postcss": "8.4.20",
		"postcss-cli": "10.1.0",
		"postcss-loader": "7.0.2",
		"prettier": "2.8.1",
		"sass-loader": "13.2.0",
		"style-loader": "3.3.1",
		"tailwindcss": "3.2.4",
		"terser-webpack-plugin": "5.3.6",
		"typescript": "4.9.4",
		"webpack": "5.75.0",
		"webpack-cli": "5.0.1",
		"webpack-livereload-plugin": "3.0.2",
		"wp-hookdoc": "0.2.0"
	},
	"packageManager": "yarn@3.2.1",
	"eslintConfig": {
		"extends": "@wcpos/eslint-config"
	}
}