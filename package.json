{
	"name": "wcpos-plugin",
	"version": "1.0.0-beta",
	"description": "A simple front-end for taking WooCommerce orders at the Point of Sale.",
	"main": "index.js",
	"sideEffects": [
		"*.css"
	],
	"scripts": {
		"wp-env": "wp-env",
		"start": "wp-env start --xdebug=debug --debug",
		"stop": "wp-env stop",
		"cli": "wp-env run cli",
		"wp": "wp-env run cli wp",
		"pretest": "wp-env start --xdebug=coverage && wp-env run composer 'install --no-interaction'",
		"lint:php": "wp-env run composer run-script lint",
		"lint": "run-s lint:*",
		"test:unit:php": "wp-env run phpunit 'phpunit -c /var/www/html/wp-content/plugins/woocommerce-pos/.phpunit.xml.dist --verbose'",
		"test:unit": "run-s test:unit:*",
		"test": "run-s test:*",
		"postinstall": "composer install",
		"build:docs": "rm -rf hookdocs/ && jsdoc -c hookdoc-conf.json",
		"build:js": "NODE_ENV=production ./node_modules/.bin/webpack",
		"start:js": "./node_modules/.bin/webpack --watch",
		"precommit": "composer dump-autoload --no-dev",
		"prepare": "husky install"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/wcpos/wcpos-plugin.git"
	},
	"keywords": [
		"wordpress",
		"woocommerce",
		"ecommerce",
		"point-of-sale"
	],
	"author": "kilbot",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/wcpos/wcpos-plugin/issues"
	},
	"homepage": "https://wcpos.com",
	"dependencies": {
		"@heroicons/react": "^1.0.4",
		"@reach/auto-id": "^0.16.0",
		"@reach/checkbox": "^0.16.0",
		"@reach/combobox": "^0.16.3",
		"@reach/dialog": "^0.16.2",
		"@reach/listbox": "^0.16.2",
		"@reach/portal": "^0.16.2",
		"@reach/tooltip": "^0.16.2",
		"@wordpress/api-fetch": "^5.2.3",
		"@wordpress/element": "^4.0.2",
		"@wordpress/i18n": "^4.2.3",
		"@wordpress/url": "^3.2.3",
		"classnames": "^2.3.1",
		"lodash": "^4.17.21",
		"react": "^17.0.2",
		"react-beautiful-dnd": "^13.1.0",
		"react-dom": "^17.0.2",
		"react-error-boundary": "^3.1.3"
	},
	"devDependencies": {
		"@babel/core": "^7.15.8",
		"@babel/plugin-transform-runtime": "^7.15.8",
		"@babel/preset-env": "^7.15.8",
		"@babel/preset-react": "^7.14.5",
		"@babel/preset-typescript": "^7.15.0",
		"@babel/runtime": "^7.15.4",
		"@svgr/webpack": "^5.5.0",
		"@types/fork-ts-checker-webpack-plugin": "^0.4.5",
		"@types/jest": "^27.0.2",
		"@types/lodash": "^4.14.175",
		"@types/react": "^17.0.30",
		"@types/react-beautiful-dnd": "^13.1.2",
		"@types/react-dom": "^17.0.10",
		"@types/wordpress__url": "^2.14.0",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"@wordpress/babel-plugin-makepot": "^4.2.0",
		"@wordpress/env": "^4.1.2",
		"autoprefixer": "^10.3.7",
		"babel-loader": "^8.2.2",
		"css-loader": "^6.4.0",
		"eslint": "8.0.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-jest": "^25.2.2",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.26.1",
		"eslint-plugin-react-hooks": "^4.2.0",
		"eslint-webpack-plugin": "^3.0.1",
		"file-loader": "^6.2.0",
		"fork-ts-checker-webpack-plugin": "^6.3.5",
		"html-webpack-plugin": "^5.4.0",
		"husky": "^7.0.2",
		"jest": "^27.3.1",
		"jsdoc": "^3.6.7",
		"mini-css-extract-plugin": "^2.4.2",
		"node-sass": "^6.0.1",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.3.9",
		"postcss-cli": "^9.0.1",
		"postcss-loader": "^6.2.0",
		"prettier": "^2.4.1",
		"sass-loader": "^12.2.0",
		"style-loader": "^3.3.0",
		"tailwindcss": "^2.2.17",
		"terser-webpack-plugin": "^5.2.4",
		"typescript": "^4.4.4",
		"webpack": "^5.59.0",
		"webpack-cli": "^4.9.1",
		"webpack-livereload-plugin": "^3.0.2",
		"wp-hookdoc": "^0.2.0"
	}
}
